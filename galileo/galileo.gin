; galileo.gin

; This file is unconditionally loaded by the galileo.exe program
; and MUST be resident in the same directory as galileo.exe
; Its purpose is to set local initialization parameters.

; default paths
data.directory          = ~\ap_stuff
script.directory        = ~\ap_scr
temporary.directory     = ~\temp
; georama.directory       = d:\georana
; html.shell.open.command = c:\goodapps\opera\opera.exe /nowin

; initial spatial index
voronoi.index           = ~\ap_stuff\world64.vix

; ellipsoid specifications
ellipsoid.number        = 34
; ellipsoid.semiaxes      = 6378137.0e0, 6356752.3141e0

angle.sexagesimal = 0

; virtual memory space
work.space.size         = 8000000
; constructing very large objects?
; mmf.out.size            = 96000000

; display dimensions (meters)
; (default values set by Windows)
display.width           = 0.310
display.height          = 0.230

; initial window layout
; console.position        = 10,47,50,10
; map.position            = 10,02,50,45
console.position        = 4,1,42,16
map.position            = 4,17,42,40

; minimum detected mouse motion
; (for sampled mouse input operations)
coincident.pixel.count  = 5

; style
console.background          = c0
console.font.face           = Terminal
console.font.size           = 0
console.text.color.input    = c196
console.text.color.info     = c65
console.text.color.warning  = c155
console.text.color.error    = c150
console.text.color.abort    = c210

; script parameters
script.echo         = 1
script.pace         = 100

; geometry
center              = n45,w95
scale               = 50e6
projection          = stereo

; map field
background          = c215

; default screen styles
coastline           = c39
cell.lines          = c81
cell.lines.boundary = c81
cell.lines.interior = c81
cell.centers        = c81m1
lat.long.grid       = c3
object.lines        = c24w1
object.points       = c3m1
object.area         = c3p9
orbit.trace         = c3
swath.trace         = c3
time.scale          = 500
object.highlight    = c150

; hard copy (monochrome) port

; HPGL default styles
hpgl.mono.frame               = c0w1
hpgl.mono.coastline           = c0w1p1
hpgl.mono.lat.long.grid       = c0w0d1

; PCL5 HP-GL/2, monochrome, page: 11x8.5" landscape, frame: 240x178mm
